}
.popupBackground
{
    background-color:black;
    opacity: 0.20;
	filter: alpha(opacity = 2);
	position: fixed; 
	width: 100%; 
	height: 100%; 
	top: 0;
	left: 0;
	z-index: 9000;
	cursor: inherit;   
	background-attachment: fixed;	
	overflow: hidden;
	overflow-y: hidden;
   
}
