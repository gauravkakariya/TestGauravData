<!--                         <input type="button" class= "btn" onclick="savePersonAccountAF();" value="Save" /> -->
<!--                         <apex:actionFunction status="saveLoading" name="savePersonAccountAF" action="{!saveAccount}" rerender="MessageElement,hidePage" /> -->
                        <apex:actionStatus id="saveLoading" onstart="loading(true)" onstop="loading(false)" /> 
                        <div id="contentLoading" style="display:none;">
                            <div style="text-align: center;">
                              <img src="/img/loading.gif" alt="Loading graphic" style="top:50px;" />
                            </div>
                        </div>
