                        <apex:actionFunction status="loading" name="getClientLeadCountAF" action="{!getClientandLeadCount}"  onComplete="displayClientandLead();return false;" />
                        <apex:outputPanel id="ShowClientCounterPanel" >
                            <script>
                            function displayClientandLead(){
                                var conStatus;
                                var isGeneralRecType = '{!isGeneral}';
                                var relatedCount = '{!relatedClientandLeadCount}';
                                var isClientCountDoneJS = '{!isClientCountDone}';
                                //alert(isClientCountDoneJS);
                                if((isGeneralRecType != null || isGeneralRecType !='') && isGeneralRecType == 'true' && relatedCount != '' )
                                {
                                    conStatus = confirm(relatedCount);
                                    if(conStatus)
                                    {
                                        callSave();
                                    }   
                                }
                            }
                            
                            function loading(val) {
                                if (val) {
                                  document.getElementById('contentLoading').style.display = 'block';
                                  document.getElementById('contentLoading').className = 'hidePage';
                                  
                                }
                                else {
                                  document.getElementById('contentLoading').style.display = 'none';
                                  document.getElementById('contentLoading').className = '';
                                }
                             }
                            </script>
                          </apex:outputPanel>
                        <apex:actionFunction name="callSave" action="{!saveAccount}" status="loading"/>
                        <apex:actionStatus id="loading" onstart="loading(true)" onstop="loading(false)" /> 
                        <div id="contentLoading" style="display:none;">
                            <div style="text-align: center;">
                              <img src="/img/loading.gif" alt="Loading graphic" style="top:50px;" />
                            </div>
                        </div>
                        
                      </apex:outputpanel>
                      
                      
