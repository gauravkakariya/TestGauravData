 <script type="text/javascript"> 
 
    
  function openLookup(baseURL, width, modified, searchParam){
    var originalbaseURL = baseURL;
    var originalwidth = width;
    var originalmodified = modified;
    var originalsearchParam = searchParam;
    
    var lookupType = baseURL.substr(baseURL.length-3, 3);
    if (modified == '1') baseURL = baseURL + searchParam;
 
    var isCustomLookup = false;
 
    // Following "001" is the lookup type for accountObj object so change this as per your standard or custom object
    if(lookupType == "001")
    {
      var urlArr = baseURL.split("&");
      var txtId = '';
      if(urlArr.length > 2) {
        urlArr = urlArr[1].split('=');
        txtId = urlArr[1];
      }
    
      var  clientRecord = document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU:BU_LookUp_Client');
      var  generalRecord = document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_General:BU_LookUp_General');
      var  masterRecord = document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_Master:BU_LookUp_Master');
      var  influencerRecord = document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_Influencer:BU_LookUp_Influencer');
      
      // Following is the url of Custom Lookup page. You need to change that accordingly
      baseURL = "/apex/AccountLookUpCustom?txt=" + txtId;
     /*
      if((clientRecord == '' ||  clientRecord ==null || clientRecord.value == '') && ('{!isClient == true}') ){
          baseURL = baseURL  + "&BU_Name=" + document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU:BU_LookUp_Client').value;
          baseURL = baseURL  + "&RecordType=" + '{!$CurrentPage.parameters.RecordType}';
      }
     */
      if((clientRecord != '' && clientRecord !=null  && clientRecord.value != '') && ('{!isClient}') ){
          baseURL = baseURL  + "&BU_Name=" + document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU:BU_LookUp_Client').value;
          baseURL = baseURL  + "&RecordType=" + '{!$CurrentPage.parameters.RecordType}';
      }
      if(generalRecord != '' && generalRecord !=null  && generalRecord.value != '' && '{!isGeneral == true}'){
          baseURL = baseURL  + "&BU_Name=" + document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_General:BU_LookUp_General').value;
          baseURL = baseURL  + "&RecordType=" + '{!$CurrentPage.parameters.RecordType}';
      }
      if(masterRecord != '' && masterRecord !=null && masterRecord.value != '' &&  '{!isMaster == true}'){
          baseURL = baseURL  + "&BU_Name=" + document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_Master:BU_LookUp_Master').value;
          baseURL = baseURL  + "&RecordType=" + '{!$CurrentPage.parameters.RecordType}';
      }
      if(influencerRecord != '' && influencerRecord !=null && influencerRecord.value != '' && '{!isInfluencer == true}'){
          baseURL = baseURL  + "&BU_Name=" + document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_Influencer:BU_LookUp_Influencer').value;
          baseURL = baseURL  + "&RecordType=" + '{!$CurrentPage.parameters.RecordType}';
      }
     
      // Following is the id of apex:form control "myForm". You need to change that accordingly
      baseURL = baseURL + "&frm=" + escapeUTF("{!$Component.myForm}");
      if (modified == '1') {
        baseURL = baseURL + "&lksearch=" + searchParam;
      }
 
      // Following is the ID of inputField that is the lookup to be customized as custom lookup
      if(txtId.indexOf('accountObj') > -1 ){
        isCustomLookup = true;
      }
    }else if(lookupType == "{!objPrefixTeamMember}")
    {
          var urlArr = baseURL.split("&");
          var txtId = '';
          if(urlArr.length > 2) {
            urlArr = urlArr[1].split('=');
            txtId = urlArr[1];
          }
        
          // Following is the url of Custom Lookup page. You need to change that accordingly
          baseURL = "/apex/TeamMemberLookup?txt=" + txtId;
          
          // Following is the ID of inputField that is the lookup to be customized as custom lookup
          if(txtId.indexOf('PST_LookUp') > -1 ){
            isCustomLookup = true;
            baseURL = baseURL + "&TM_Type={!$Label.Team_Member_Type_PST}";
          }else if(txtId.indexOf('MST_LookUp') > -1){
             isCustomLookup = true;
            baseURL = baseURL + "&TM_Type={!$Label.Team_Member_Type_MST}";
          }else if(txtId.indexOf('MST2_LookUp') > -1){
             isCustomLookup = true;
            baseURL = baseURL + "&TM_Type={!$Label.Team_Member_Type_MST2}";
          }else if(txtId.indexOf('FPA_LookUp') > -1){
             isCustomLookup = true;
            baseURL = baseURL + "&TM_Type={!$Label.Team_Member_Type_FPA}";
          }
          
          
          if(document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU:BU_LookUp_Client') != null && document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU:BU_LookUp_Client').value != ''){
              baseURL = baseURL  + "&BU_Name=" + document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU:BU_LookUp_Client').value;
          }
          else if(document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_General:BU_LookUp_General') != null && document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_General:BU_LookUp_General').value != ''){
              baseURL = baseURL  + "&BU_Name=" + document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_General:BU_LookUp_General').value;
              baseURL = baseURL  + "&RecordType=" + '{!$CurrentPage.parameters.RecordType}';
             }
          else if(document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_Master:BU_LookUp_Master') != null && document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_Master:BU_LookUp_Master').value != ''){
              baseURL = baseURL  + "&BU_Name=" + document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_Master:BU_LookUp_Master').value;
              baseURL = baseURL  + "&RecordType=" + '{!$CurrentPage.parameters.RecordType}';
          }
          else if(document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_Influencer:BU_LookUp_Influencer') != null && document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_Influencer:BU_LookUp_Influencer').value != ''){
              baseURL = baseURL  + "&BU_Name=" + document.getElementById('Page:myForm:PageBlock:PageBlockSection_BU_Influencer:BU_LookUp_Influencer').value;
              baseURL = baseURL  + "&RecordType=" + '{!$CurrentPage.parameters.RecordType}';
          }
        
          // Following is the id of apex:form control "myForm". You need to change that accordingly
          baseURL = baseURL + "&frm=" + escapeUTF("{!$Component.myForm}");
          if (modified == '1') {
            baseURL = baseURL + "&lksearch=" + searchParam;
          }
  
    }
 
 
    if(isCustomLookup == true){
      openPopup(baseURL, "lookup", 350, 480, "width="+width+",height=480,toolbar=no,status=no,directories=no,menubar=no,resizable=yes,scrollable=no", true);
    }
    else {
      if (modified == '1') originalbaseURL = originalbaseURL + originalsearchParam;
      openPopup(originalbaseURL, "lookup", 350, 480, "width="+originalwidth+",height=480,toolbar=no,status=no,directories=no,menubar=no,resizable=yes,scrollable=no", true);
    } 
  }
</script>

