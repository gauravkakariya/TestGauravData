    <validationRules>
        <fullName>ChequeSubmissionDateValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(Cheque_Submission_date__c),NOT(ISBLANK(Target_Revenue__c)))</errorConditionFormula>
        <errorDisplayField>Cheque_Submission_date__c</errorDisplayField>
        <errorMessage>Please Enter Cheque Submission Date once Target Revenue is entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GreaterChequeSubmissionDate</fullName>
        <active>true</active>
        <errorConditionFormula>Cheque_Submission_date__c &lt; Next_renewal_date__c</errorConditionFormula>
        <errorDisplayField>Cheque_Submission_date__c</errorDisplayField>
        <errorMessage>Cheque Submission Date should be greater than Next Renewal Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Target_Revenue</fullName>
        <active>true</active>
        <errorConditionFormula>AND(AND(NOT(ISBLANK(Cheque_Submission_date__c)),Cheque_Submission_date__c &gt;=  Next_renewal_date__c),ISBLANK(Target_Revenue__c))</errorConditionFormula>
        <errorDisplayField>Target_Revenue__c</errorDisplayField>
        <errorMessage>Please enter Target Revenue once Cheque Submission date is entered</errorMessage>
    </validationRules>
