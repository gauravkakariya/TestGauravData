    <fields>
        <fullName>Annual_Premium_amount__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(  
     Premium_Frequency__c ,
      &apos;Annually&apos;,  Premium_Amount_Rs__c ,
     &apos;Semi Annual&apos;, (Premium_Amount_Rs__c  * 2), 
     &apos;Quarterly&apos;,( Premium_Amount_Rs__c  * 4), 
     &apos;Monthly&apos;,(Premium_Amount_Rs__c   * 12),
    0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Annual_Premium_amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Annualized_Premium_In_Rs__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
 CONTAINS($RecordType.Name , &apos;Life Insurance&apos;),
  CASE(  Premium_Frequency__c ,&apos;Annually&apos;, Death_Benefit_Rs__c ,
     &apos;Semi Annual&apos;, (Death_Benefit_Rs__c  * 2), 
     &apos;Quarterly&apos;,( Death_Benefit_Rs__c  * 4), 
     &apos;One Time Premium&apos;,(Death_Benefit_Rs__c  * 12),
     &apos;Monthly&apos;,(Death_Benefit_Rs__c  * 12),
     0
  ),
 IF(CONTAINS($RecordType.Name , &apos;General Insurance&apos;), 
  CASE( 
  Premium_Frequency__c ,&apos;Annually&apos;, Sum_Assured_Rs__c ,
     &apos;Semi Annual&apos;, (Sum_Assured_Rs__c  * 2), 
     &apos;Quarterly&apos;,( Sum_Assured_Rs__c * 4), 
     &apos;One Time Premium&apos;,(Sum_Assured_Rs__c * 12),
     &apos;Monthly&apos;,(Sum_Assured_Rs__c  * 12),
     0
   ),0
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Annualized Premium (Rs.)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>
