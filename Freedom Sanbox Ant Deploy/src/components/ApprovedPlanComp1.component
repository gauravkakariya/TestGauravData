          <td class="textStyle" style="text-align:right;">{!ap.allocated}</td>
          <apex:repeat value="{!ap.golListVals}" var="gtObj">
              <td class="textStyle" style="text-align:right;">{!gtObj}</td>
          </apex:repeat>
