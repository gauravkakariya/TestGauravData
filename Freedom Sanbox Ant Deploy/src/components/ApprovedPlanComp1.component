	        var totalCount=0;
	        for(var j=0;j<splitid.length-2;j++)
	        {
	        	strid +=splitid[j]+':';
	        }
	        for(var i=0;i<=25;i++)
	        {
	        	if(document.getElementById(strid+i+':'+txtId)==null)
	        	break;
	        	totalCount = totalCount + Number(document.getElementById(strid+i+':'+txtId).value);
	        }
	        if(totalCount >100)
	        {
	       		alert('Values are greater then 100');
	        }
	    } 
