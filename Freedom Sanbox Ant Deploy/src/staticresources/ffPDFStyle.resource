.popupForeground
{
    Z-INDEX: 1;
    BORDER-BOTTOM: 2px solid;
    POSITION: absolute;
    BORDER-LEFT: 2px solid;
    PADDING-BOTTOM: 1px;
    BACKGROUND-COLOR: white;
    PADDING-LEFT: 1px;
    WIDTH: 90%;
    PADDING-RIGHT: 1px;
    MARGIN-LEFT: 0%;
    BORDER-TOP: 2px solid;
    TOP: 30px;
    BORDER-RIGHT: 2px solid;
    PADDING-TOP: 1px
}
.popupBackground
{
    Z-INDEX: 0;
    POSITION: absolute;
    FILTER: alpha(opacity = 20);
    BACKGROUND-COLOR: black;
    WIDTH: 103%;
    HEIGHT: 100%;
    MARGIN-LEFT: 0%;
    TOP: 0px;
    LEFT: -1%;
    opacity: 0.40
}
