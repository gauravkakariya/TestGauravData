table.tableClass tr:nth-child(odd) td
{
    BACKGROUND-COLOR: white;
    FONT-FAMILY: Calibri !important;
    FONT-SIZE: 14px;
}
table.tableClass tr:nth-child(even) td
{
    BACKGROUND-COLOR: #e4fbc5;
    FONT-FAMILY: Calibri !important;
    FONT-SIZE: 14px;
}
.tableData  tr:nth-child(even) td
{
    BACKGROUND-COLOR: #e4fbc5;
    FONT-FAMILY: Calibri !important;
    FONT-SIZE: 14px;
}
table.tableClass tr:first-child td
{
    BACKGROUND-COLOR: #66cd00;
    FONT-WEIGHT: bold;
    FONT-FAMILY: Calibri !important;
    FONT-SIZE: 14px;
    COLOR: white;
}