	padding:0em 0em 0.0em 0.0em;
}

/* Commented Backslash Hack hides rule from IE5-Mac \*/
#menu1  ul li a {
	float: none;
}

 /*End IE5-Mac hack */
#menu1  ul.level2, #menu1 ul.level3 {
	position:absolute;
	top: 0px;
	left: 0px;
	visibility: hidden;
	border-left: 1px solid #fff;
	border-top: 1px solid #fff;
	border-right: 1px solid #fff;
	background:#eaeaea;
	display:block;
	width:125px;
}

#menu1  ul.level2 li, #menu1 ul.level3 li {
	float: none;
	margin: 0;
	padding: 0;
	display:block;
	width:auto;
	
}

#menu1  ul.level2 li a, #menu1 ul.level3 li a {
	text-decoration: none;
	font-weight:normal;
	color:#2f4f6b;
	padding: 5px 9px 5px 5px;
	display:block;
	width:auto;
}

#menu1  ul.level2 li a:hover,#menu1  ul.level3 li a:hover {
	text-decoration: none;
	color:#FFF;
	font-weight:normal;
	background-color:#f58220;
	background-image: none;
	display:block;
	width:auto;
}


