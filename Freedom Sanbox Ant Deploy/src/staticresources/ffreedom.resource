	font-family: Verdana;
}
#bottomcontener  ul {
	display: block;
	margin-top: 10px;
	padding-top: 0px;
	line-height: 1em;
	list-style: none;
	z-index: 90
}

#bottomcontener  ul li {
	float: right;
	display:inline;
	margin: 5px 3px 0 0;
	padding: 0;
	font-size: 12px;
	line-height: 0em;
	list-style-type: none;
}

#bottomcontener ul li a {
	float: left;
	display: inline;
	color:#2f4f6b;
	width: auto;
	font-weight: normal;
	background: transparent;
	text-decoration: none;
	margin: 0;
	/*padding: 0.9em 0.8em 0.5em 1em;*/
	padding: 1.5em 0.8em 0em 1em;
}
.leftcontainer {
	width: 953px;
	/*float: left;*/
	background-color:#FFF;
	overflow:hidden;
}
#bottomcontener  ul li a:hover {
	color:#f58220;
	text-decoration: none;
}

#bottomcontener  ul li.sep {
	
	padding: 0em 0em 0.5em 0em;
}

 /*Commented Backslash Hack hides rule from IE5-Mac \*/
#bottomcontener  ul li a {
	float: none;
}

/* End IE5-Mac hack */
#bottomcontener  ul.level2, #bottomcontener ul.level3 {
	position: absolute;
	top: 0px;
	left: 0px;
	visibility: hidden;
	border-left: 1px solid #fff;
	border-top: 1px solid #fff;
	border-right: 1px solid #fff;
	background:#FFF;
}

#bottomcontener  ul.level2 li, #bottomcontener ul.level3 li {
	float: none;
	margin: 0;
	padding: 0;
	
}

#bottomcontener  ul.level2 li a, #bottomcontener ul.level3 li a {
	text-decoration: none;
	font-weight:normal;
	color:#f58220;
	padding: 5px 9px 5px 5px;
}

#bottomcontener  ul.level2 li a:hover,#bottomcontener  ul.level3 li a:hover {
	font-weight: normal;
	color:#2f4f6b;
	/*background-color:#f58220;*/
	background-image: none;
	padding: 3px 9px 3px 5px;
}

